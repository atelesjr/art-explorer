{
	"compilerOptions": {
		"composite": true,
		"declaration": true,
		"declarationMap": true,
		"emitDeclarationOnly": true,
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"target": "ES2022",
		"lib": ["ES2022"],
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true
	},
	"include": ["vite.config.ts"]
}
